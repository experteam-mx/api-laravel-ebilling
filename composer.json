{
  "name": "experteam/api-laravel-ebilling",
  "description": "Laravel library to manage common operations used in electronic billing processes",
  "homepage": "https://github.com/experteam-mx/api-laravel-ebilling",
  "keywords": [
    "laravel",
    "ebilling",
    "experteam"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jefferson Morocho",
      "email": "jefferson.morocho@experteam.com.ec",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Experteam\\ApiLaravelEBilling\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Experteam\\ApiLaravelEBilling\\ApiLaravelEBillingServiceProvider"
      ],
      "aliases": {
        "DocumentRequestManager": "Experteam\\ApiLaravelEBilling\\Facades\\DocumentRequestManagerFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1 || ^8.2",
    "ext-json": "*",
    "illuminate/database": "^9.0 || ^10.0 || ^11.0",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0",
    "shalvah/laravel-jsend": "^2.1",
    "ext-simplexml": "*"
  }
}
